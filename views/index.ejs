<% include partials/header %>

<h1>Welcome to the blog page listing</h1>

<% blogs.forEach(function(blog){ %>
<div>
    <h3><%= blog.title %></h3>
    <img src="<%= blog.image %>">
    <p><%= blog.body %></p>
    <p><%= blog.created %></p>
    <a href="/blogs/<%= blog._id %>">Show more</a>
</div>
<% }); %>

<% include partials/footer %>